name = input('Student Name:')
print ('\nPlease input'+ '' + name + "'s grades \n")

sci = float(input('enter grade:'))
math = float(input('enter grade:'))
fil = float(input('enter grade:'))
it = float(input('enter grade:'))
eng = float(input('enter grade:'))

sum=(sci+math+fil+it+eng)
avg=sum/5

print(name + "'s average is " + str(avg))

if avg >=85:
    print('with highest honor')
elif avg >=80 and avg<=84:
    print('with honor')
elif avg >=75 and avg<=79:
    print('above pass')
else:
    print('fail')

